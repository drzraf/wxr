<xs:schema
		xmlns:xs='http://www.w3.org/2001/XMLSchema'
		elementFormDefault='qualified'
		targetNamespace='&RSS_Content_Module_NamespaceURI;'
		version='&RSS_Content_Module_Version;'>
	<xs:annotation>
		<xs:documentation>
			This XML Schema 1.1 schema represents the subset of "RDF Site Summary 1.0 Modules: Content" used by WXR &WXR_Version;. 
		</xs:documentation>
